 <main>
    <h1>Welcome to {{name}}'s Portfolio</h1>
    <p>{{description}}</p>

    <section id="latest-projects">
      <h2>Latest Projects</h2>
      <ul>
        {{#each projects}}
          <li>
            <h3>{{this.title}}</h3>
            <p>{{this.description}}</p>
            <a href="{{this.url}}">View Project</a>
          </li>
        {{/each}}
      </ul>
    </section>
  </main>

  <footer>
    <p>&copy; {{year}} {{name}}. All rights reserved.</p>
  </footer>
</body>
</html>